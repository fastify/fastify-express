  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-express/issues"
  },
  "homepage": "https://github.com/fastify/fastify-express#readme",
  "devDependencies": {
    "@fastify/formbody": "^8.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "*",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "fastify": "^5.0.0",
    "helmet": "^8.0.0",
    "neostandard": "^0.11.9",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "serve-static": "^1.15.0",
    "simple-get": "^4.0.1",
    "tap": "^21.0.0",
    "tsd": "^0.31.0"
  },
